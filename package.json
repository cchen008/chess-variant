{
  "name": "mvp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.34",
    "@types/date-fns": "^2.6.0",
    "aws-amplify": "^1.2.4",
    "aws-amplify-react": "^2.5.4",
    "bootstrap": "^4.3.1",
    "chess.js": "0.10.2",
    "chessboardjsx": "github:seyson/chessboardjsx",
    "date-fns": "^2.8.0",
    "font-awesome": "^4.7.0",
    "material-table": "^1.54.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-chat-window": "^1.2.1",
    "react-countdown-now": "^2.1.2",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-responsive-navbar": "^1.0.11",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "react-scripts test --watchAll=false  --detectOpenHandles",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:update": "react-scripts test --updateSnapshot --watchAll=false",
    "coveralls": "react-scripts test --coverage --watchAll=false && cat ./coverage/lcov.info | coveralls",
    "eject": "react-scripts eject",
    "postinstall": "cp src/chess/chess.js node_modules/chess.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/*.stories.{js,jsx}",
      "!src/**/index.{js,jsx}",
      "!src/config/**"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^9.4.3",
    "puppeteer": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
